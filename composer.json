{
    "name": "genericmilk/docudoodle",
    "description": "Generate documentation for your Laravel application using OpenAI",
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Docudoodle\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Docudoodle\\DocudoodleServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your-email@example.com"
        }
    ],
    "homepage": "https://github.com/your-vendor/docudoodle",
    "keywords": [
        "documentation",
        "php",
        "openai",
        "generator"
    ]
}
